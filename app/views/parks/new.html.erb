<h1 class="text-center">Add a New Park</h1>
<h3 class="text-center">More protected land</h3>

<div class="container">
	<%= form_for @park do |f| %>
		<div class="form-group">
			<%= f.label :name %>
			<%= f.text_field :name, :class => "form-control" %>
		</div>
		<div class="form-group">
			<%= f.label :description %>
			<%= f.text_area :description, :class => "form-control" %>
		</div>
		<div class="form-group">
			<%= f.label :picture %>
			<%= f.text_field :picture, :class => "form-control" %>
		</div>
		<div class="form-group">
			<%= f.collection_check_boxes :ranger_ids, @rangers, :id, :name %>
			
		</div>
		<%= f.submit :class => "btn btn-success" %>

	<% end %>
</div>